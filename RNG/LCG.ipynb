        "# Linear Congruential Generator"
      ],
      "metadata": {
        "id": "k3ArofV64mg2"
      }
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "WUshk2ZA4j_5"
      },
      "outputs": [],
      "source": [
        "def lcg():\n",
        "\n",
        "    r = 88675123\n",
        "    a = 1140671485\n",
        "    c = 128201163\n",
        "    m = 2**24\n",
        "\n",
        "    def _random():\n",
        "        nonlocal a, c, m, r\n",
        "        r = (a*r + c) % m\n",
        "        return r\n",
        "\n",
        "    return _random"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "r = lcg();\n",
        "\n",
        "for i in range(10):\n",
        "    print(r())"
      ],
